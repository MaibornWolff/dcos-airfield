{
    "name": "airfield",
    "version": "1.0.0",
    "description": "Airfield frontend",
    "private": true,
    "scripts": {
        "dev": "concurrently --kill-others \"npm run mock\" \"npm run dev-mock\"",
        "dev-mock": "webpack-dev-server --inline --hot --quiet --open --config webpack/webpack.dev-mock.conf.js",
        "dev-server": "webpack-dev-server --inline --hot --quiet --open --config webpack/webpack.dev-server.conf.js",
        "mock": "nodemon mock/index.js --watch mock -e js",
        "build": "node webpack/build",
        "lint": "eslint --ext js,vue ./**/*.{js,vue}"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "1.2.3",
        "@fortawesome/free-brands-svg-icons": "5.3.0",
        "@fortawesome/free-solid-svg-icons": "5.3.0",
        "@fortawesome/vue-fontawesome": "0.1.1",
        "axios": "0.19.0",
        "bootstrap-vue": "2.0.0-rc.22",
        "moment": "2.24.0",
        "vue": "2.5.17",
        "vue-router": "3.0.1",
        "vuex": "3.0.1"
    },
    "devDependencies": {
        "babel-core": "6.26.3",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.1.1",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-stage-2": "6.24.1",
        "babel-runtime": "6.26.0",
        "concurrently": "4.0.1",
        "css-loader": "0.28.4",
        "eslint": "4.18.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-vue": "4.7.1",
        "express": "4.16.4",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.29.0",
        "node-sass": "4.12.0",
        "nodemon": "1.18.3",
        "optimize-css-assets-webpack-plugin": "2.0.0",
        "rimraf": "2.6.1",
        "sass-loader": "6.0.6",
        "style-loader": "0.18.2",
        "uglify-es": "3.3.9",
        "uglifyjs-webpack-plugin": "1.2.4",
        "url-loader": "0.5.9",
        "vue-hot-reload-api": "2.1.0",
        "vue-html-loader": "1.2.4",
        "vue-loader": "13.0.1",
        "vue-template-compiler": "2.5.17",
        "webpack": "3.10.0",
        "webpack-dev-server": "2.9.7"
    }
